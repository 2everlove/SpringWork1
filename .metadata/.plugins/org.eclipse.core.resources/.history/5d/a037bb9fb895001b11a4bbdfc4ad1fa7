package jmp.spring.ex01;

import static org.junit.Assert.assertEquals;

import org.junit.Test;
public class JunitTest {
	int count;
	int[] result = new int[3];
	@Test
	public void testA() {
		Calculator cal = new Calculator();
		int res = cal.calculator("5","5","+");
		result[count++] = res;
		assertEquals(10, res);
	}
	@Test
	public void testB() {
		Calculator cal = new Calculator();
		int res = cal.calculator("5","5","+");
		
		assertEquals(5, res);
	}
	@Test
	public void testC() {
		Calculator cal = new Calculator();
		
		int[] ex = {10,5,5};
		
		int res = cal.calculator("String","5","+");
		
		assertEquals(5, res);
	}
}
